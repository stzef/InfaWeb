{
    "env": {
        "browser": true,
        "jquery" : true
    },
    "globals": {
        "reset_form_fac" : true,
        "customValidationInput" : true,
        "currencyFormat" : true,
        "data_validation" : true,
        "alertBootstrap" : true,
        "loading_animation" : true,
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [
            2,
            "tab"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "quotes": [
            2,
            "double"
        ],
        "semi": [
            2,
            "always"
        ]
    }
}
