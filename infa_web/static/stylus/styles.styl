.ae
	&-img-list
		height 50px
		width auto
		
.errorlist
	margin 0
	li 
		list-style none
	.alert
		margin-bottom 5px

a[disabled]
	pointer-events none
	
.html_tooltip
	font-size 16px

.loading-animation
	width 100%
	height 100%
	top 0
	bottom 0
	position fixed
	background-color rgba(0, 0, 0, 0.54)
	z-index 100
	transition 1s

.loading
	margin 300px auto
	padding 10px
	width 200px
	background-color white
	border-radius 3px
	transition 1.2s